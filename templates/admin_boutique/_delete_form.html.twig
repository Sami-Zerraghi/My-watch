<form method="post" action="{{ path('app_admin_boutique_delete', {'id': boutique.id}) }}" onsubmit="return confirm('Etes vous sur de vouloir supprimer cette boutique?');" class ="d-inline">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ boutique.id) }}">
    <button class="btn btn-danger"><i class="align-middle" data-feather= "trash"></i></button>
</form>
